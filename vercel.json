{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/dashboard",
      "destination": "https://app.theedgelatam.io/dashboard",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" }
      ]
    }
  ]
}
